<?xml version="1.0"?>
<!--
/**
 * Template import
 *
 * @author Manish Jain
 * @author Fabrizio Branca
 */
-->
<config>
    <modules>
        <Aoe_TemplateImport>
            <version>0.4.0</version>
        </Aoe_TemplateImport>
    </modules>
    <global>
        <blocks>
            <aoe_templateimport>
                <class>Aoe_TemplateImport_Block</class>
            </aoe_templateimport>
            <page>
                <rewrite>
                    <html>Aoe_TemplateImport_Block_Html</html>
                </rewrite>
            </page>
        </blocks>
        <helpers>
            <aoe_templateimport>
                <class>Aoe_TemplateImport_Helper</class>
            </aoe_templateimport>
        </helpers>
        <models>
            <aoe_templateimport>
                <class>Aoe_TemplateImport_Model</class>
                <resourceModel>aoe_templateimport_resource</resourceModel>
            </aoe_templateimport>
            <aoe_templateimport_resource>
                <class>Aoe_TemplateImport_Model_Resource</class>
                <entities>
                    <config>
                        <table>aoe_templateimport_config</table>
                    </config>
                </entities>
            </aoe_templateimport_resource>
        </models>
        <resources>
            <aoe_templateimport_setup>
                <setup>
                    <module>Aoe_TemplateImport</module>
                </setup>
            </aoe_templateimport_setup>
        </resources>
    </global>
</config>
